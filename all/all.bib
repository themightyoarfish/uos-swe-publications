@InProceedings{noyer:tool:14:a,
    author = {Noyer, Arne and Iyenghar, Padma and Pulverm{\"u}ller, Elke and Pramme, Florian and Engelhardt, Joachim and Samson, Benjamin and Bikker, Gert},
    title = "Tool Independent Code Generation for the UML - Closing the Gap Between Proprietary Models and the Standardized UML Model",
    booktitle = "Proceedings of the 9th International Conference on Evaluation of Novel Software Approaches to Software Engineering (ENASE 2014)",
    publisher = "SCITEPRESS",
    volume = "",
    month = "April",
    year = "2014",
    pages = "117 -- 125",
    location = "Lisbon, Portugal",
    mytype = "CONFERENCE",
    abstract = "Embedded software development is moving towards the model-based paradigm to support the complexity of today‚Äôs embedded systems, as they become more and more important and omnipresent in our daily lives. In this context, the Unified Modeling Language (UML) is a widely used standard. Code generators can be executed to generate source code from UML models. Usually the code generators are proprietary for one UML tool. If code generators for different targets or programming languages have to be supported by various modeling tools, the wheel must be reinvented. Code generators could use the standardized Extensible Markup Language Metadata Interchange (XMI) format of the UML as a basis. However, tools export their data to XMI differently. Therefore, the paper shows how the proprietary models of UML tools can be mapped to a standardized UML model. This is realized by using techniques for model to model transformations. These techniques need a meta-model for the source and the target model. Hence, an approach is introduced for creating meta-models for Application Programming Interfaces (APIs) of UML-tools, which act as a facade. Then the code generators can work with the standardized UML model to generate the source code. This results in an improved scalability of the code generators.",
    language = "English",
    url_home = "http://dx.doi.org/10.5220/0004870701170125",
    key = "noyer:tool:14",
    publipy_biburl = "../Publikationssammlung/all/bib/noyer:tool:14.bib",
    publipy_pdfurl = "../Publikationssammlung/all/pdf/noyer:tool:14.pdf",
    publipy_abstracturl = "../Publikationssammlung/all/abstracts/noyer:tool:14.txt"
}

