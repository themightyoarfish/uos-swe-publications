@InProceedings{pulvermuller:capability:07,
    author = {Pulverm{\"u}ller, Elke and Ludwig, Andr{\'e} and Belter, Roman and Zyskowski, Dominik and Heindl, Ulrich and Nguyen, Xuan Thang},
