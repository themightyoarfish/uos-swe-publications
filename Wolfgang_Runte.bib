
@InProceedings{	  krebs.wagner.ea:recognizing,
  author	= {Krebs, Thorsten and Wagner, Thomas and Runte, Wolfgang},
  title		= {{R}ecognizing {U}ser {I}ntentions in {I}ncremental
		  {C}onfiguration {P}rocesses},
  booktitle	= {Proceedings of the Workshop on Configuration at the 18th
		  International Joint Conference on Artificial Intelligence
		  (IJCAI 2003)},
  year		= {2003},
  editor	= {Mailharro, Daniel},
  pages		= {44--50},
  address	= {Acapulco, Mexico},
  month		= aug # { 11,},
  abstract	= {Increasing market demands concerning customer requirements
		  and market pressure from competitors force several
		  enterprises to diversify their product range. Offering more
		  and more product variants has the implicit impact of
		  growing development process complexity. Although many
		  configuration problems can be solved with knowledge-based
		  configuration, the usage of configuration tools is very
		  often restricted to domain experts. In this paper we extend
		  knowledge-based configuration with a novel plan recognition
		  approach. We show that the combination of configuration and
		  plan recognition simplifies the use of configuration tools
		  for non-configuration experts and non-domain experts.},
  citeseerurl	= {http://citeseer.ist.psu.edu/636146.html},
  owner		= {woru},
  timestamp	= {2009.04.02},
  mytype	= {WORKSHOP},
  url		= {http://www.inf.uos.de/woru/pub/krebs03ijcai.pdf},
  url2		= {http://citeseerx.ist.psu.edu/viewdoc/summary?doi=10.1.1.9.9482},
  url_alt1	= {http://kogs-www.informatik.uni-hamburg.de/publikationen/pub-krebs/IJCAI2003.pdf},
  url_alt2	= {http://www2.ilog.com/ijcai-03/Papers/IJCAI03-08.pdf},
  url_home	= {http://www2.ilog.com/ijcai-03/index.html}
}

@InProceedings{	  runte.el-kharbili:constraint,
  author	= {Runte, Wolfgang and El Kharbili, Marwane},
  title		= {{C}onstraint {C}hecking for {B}usiness {P}rocess
		  {M}anagement},
  booktitle	= {INFORMATIK 2009: Im Focus das Leben. Beitr{\"a}ge der 38.
		  Jahrestagung der Gesellschaft f{\"u}r Informatik e.V. (GI),
		  Workshop on Business Process Modeling and Realization
		  (BPMR-GI 2009), 28.9. - 2.10.2009, L{\"u}beck, Proceedings},
  year		= {2009},
  editor	= {Fischer, Stefan and Maehle, Erik and Reischuk,
		  R{\"u}diger},
  volume	= {P-154},
  series	= {GI-Edition - Lecture Notes in Informatics (LNI)},
  pages		= {4093--4103},
  address	= {Bonn, Germany},
  month		= sep,
  publisher	= {Gesellschaft f{\"u}r Informatik},
  abstract	= {Compliance management tackles issues related to both
		  modelling and enforcement of business constraints in
		  enterprises. In the context of business process management,
		  we propose and describe the use of constraint satisfaction
		  problems as a formal mean for representing these business
		  constraints. We propose a multi-level constraint
		  satisfaction approach to handle different levels of
		  abstraction in business process modelling. We elaborate on
		  the modelling of compliance requirements using constraint
		  satisfaction and discuss implications of this technique
		  such as problem solving strategies. This work shows that a
		  certain class of compliance problems which can be grounded
		  to constraint satisfaction problems can be solved
		  efficiently using the proposed approach.},
  isbn		= {978-3-88579-248-2},
  issn		= {1617-5468},
  owner		= {woru},
  timestamp	= {2009.10.07},
  mytype	= {WORKSHOP},
  url		= {http://www.inf.uos.de/woru/pub/runte09bpmr-gi.pdf},
  url2		= {http://www.informatik2009.de/},
  url3		= {http://www.informatik2009.de/workshops0.html},
  url4		= {http://www.informatik2009.de/fileadmin/informatik2009/Download/programmheft-informatik2009.pdf},
  url5		= {http://www.gi-ev.de/service/publikationen/lni/},
  url6		= {http://www.gi-ev.de/service/publikationen/lni/gi-edition-proceedings-2009/gi-edition-lecture-notes-in-informatics-lni-p-154.html},
  url_home	= {http://www.bit.informatik.uni-kiel.de/bpmr-gi2009/},
  url_slides	= {http://www.inf.uos.de/woru/pub/runte09bpmr-gi_slides.pdf}
}

@InProceedings{	  runte:hybrides,
  author	= {Runte, Wolfgang},
  title		= {{E}in hybrides {F}ramework f{\"u}r {C}onstraint-{S}olver
		  zur {U}nterst{\"u}tzung wissensbasierter {K}onfigurierung},
  booktitle	= {Proceedings of the 21th Workshop Planen, Scheduling und
		  Konfigurieren, Entwerfen (PuK 2007) at the 30th Annual
		  German Conference on AI (KI 2007)},
  year		= {2007},
  editor	= {Edelkamp, Stefan and Sauer, J{\"u}rgen and Schattenberg,
		  Bernd},
  pages		= {78--92},
  address	= {Osnabr{\"u}ck, Germany},
  month		= sep # { 10,},
  abstract	= {In wissensbasierten Konfigurierungssystemen werden
		  Komponenten zur Constraint-Verarbeitung f{\"u}r die
		  Verwaltung von Abh{\"a}ngigkeiten w{\"a}hrend des
		  Konfigurierungsprozesses eingesetzt. Die Effizienz von
		  Constraint-L{\"o}sungsverfahren ist allerdings stark
		  problemabh{\"a}ngig. Zudem kann es aufgrund einer Vielzahl
		  unterschiedlicher Dom{\"a}nen und der Vielf{\"a}ltigkeit
		  der Anwendungsszenarien notwendig werden, an die jeweilige
		  Dom{\"a}ne angepasste Constraint-L{\"o}sungsverfahren
		  einzusetzen. Zur Behandlung von unterschiedlichen
		  Constraint-Dom{\"a}nen innerhalb des strukturbasierten
		  Konfigurierungswerkzeugs EngCon sind Constraint-Solver
		  sowohl f{\"u}r finite als auch infinite Dom{\"a}nen
		  erforderlich. Die Steuerung muss durch eine Komponente
		  geleistet werden, mit der sich unterschiedliche
		  Constraint-Solver je nach Bedarf und dom{\"a}nenspezifisch
		  einsetzen lassen. Die Modularit{\"a}t ist dabei
		  entscheidend f{\"u}r die Austauschbarkeit einzelner
		  Komponenten. Es stellt daher einen sinnvollen Ansatz dar,
		  ein modulares Constraint-Framework einzusetzen, in das je
		  nach Bedarf, und dem in der jeweiligen Wissensbasis
		  definiertem Problem, unterschiedliche Constraint-Solver mit
		  jeweils f{\"u}r das spezifische Problem effizienten
		  L{\"o}sungsverfahren eingesetzt werden k{\"o}nnen. Das
		  Framework wird durch einen strategiebasierten Ansatz
		  unterst{\"u}tzt, der eine flexible Kooperation
		  unterschiedlicher Constraint-L{\"o}sungsalgorithmen erlaubt.},
  owner		= {woru},
  timestamp	= {2009.04.02},
  mytype	= {WORKSHOP},
  url		= {http://www.inf.uos.de/woru/pub/runte07framework.pdf},
  url2		= {http://www-is.informatik.uni-oldenburg.de/~sauer/puk/puk.html},
  url3		= {http://www.ki2007.uos.de/},
  url4		= {http://www.ki2007.uos.de/workshops.html#W3},
  url5		= {http://konferenz.kuenstliche-intelligenz.de/},
  url_alt1	= {http://www-is.informatik.uni-oldenburg.de/~sauer/puk2007/paper/PuK07-Proceedings.pdf},
  url_home	= {http://www-is.informatik.uni-oldenburg.de/~sauer/puk2007/},
  url_slides	= {http://www.inf.uos.de/woru/pub/runte07framework_slides.pdf}
}

