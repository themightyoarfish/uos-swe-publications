@InProceedings{	  goers.heuer:definition,
  author	= {G{\"o}ers, Jutta and Heuer, Andreas},
  title		= {Definition and {A}pplication of {M}etaclasses in an
		  {O}bject-{O}riented {D}atabase {M}odel},
  booktitle	= {Proc. of the 9. Int. Conf. on Data Engineering},
  year		= {1993},
  address	= {Wien, {\"O}sterreich},
  abstract	= {The paper presents the meta level concepts for an
		  object-oriented database model. Many object-oriented
		  database systems have no means to store and maintain meta
		  information in terms of the underlying database model.
		  Systems including a metaclass concept are either only
		  implicitly supporting the management of meta information or
		  explicitly giving unrestricted access to manipulate
		  metaclasses which can result in inconsistent states of the
		  system. To make the explicit support more
		  system-controlled, we use a partition of the meta scheme
		  into two parts: the system view and the application view.
		  We describe possible schema level operations and their
		  effect on metaclasses. It is shown how object-oriented
		  concepts like methods and their implementations can be
		  managed as a special kind of meta information.
		  Additionally, a simple extension to the query algebra of
		  the underlying object-oriented database system is presented
		  that allows users to query objects and meta objects within
		  a single algebra expression.},
  mytype	= {CONFERENCE}
}

